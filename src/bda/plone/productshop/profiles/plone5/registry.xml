<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="bda.plone.shop">

  <records prefix="plone.bundles/productshop"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="compile">False</value>
    <value key="depends">plone,bda-plone-shop</value>
    <value key="merge_with">default</value>
    <value key="csscompilation">++resource++productshop.css</value>
    <value key="jscompilation">++resource++productshop.js</value>
  </records>

</registry>
